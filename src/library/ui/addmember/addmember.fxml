<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="302.0" prefWidth="600.0" styleClass="root" stylesheets="@../support/common.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.ui.addmember.AddmemberController">
   <children>
      <VBox prefHeight="343.0" prefWidth="600.0">
         <children>
            <JFXTextField fx:id="name" labelFloat="true" promptText="Name" stylesheets="@../support/common.css">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font name="Comic Sans MS Bold" size="14.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="id" labelFloat="true" layoutX="10.0" layoutY="10.0" promptText="member ID" stylesheets="@../support/common.css">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font name="Comic Sans MS Bold" size="14.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="mobile" labelFloat="true" layoutX="10.0" layoutY="10.0" promptText="mobile" stylesheets="@../support/common.css">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font name="Comic Sans MS Bold" size="14.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="email" labelFloat="true" layoutX="10.0" layoutY="35.0" onAction="#addMember" promptText="Email" stylesheets="@../support/common.css">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font name="Comic Sans MS Bold" size="14.0" />
               </font>
            </JFXTextField>
            <HBox prefHeight="58.0" prefWidth="560.0">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <children>
                  <JFXButton fx:id="clear" buttonType="RAISED" onAction="#clear" prefWidth="300.0" ripplerFill="#188630" styleClass="login-button" stylesheets="@../support/common.css" text="Clear">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Courier New Bold" size="13.0" />
                     </font>
                     <effect>
                        <Glow />
                     </effect>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <graphic>
                        <FontAwesomeIconView glyphName="ARROW_CIRCLE_LEFT" size="20" />
                     </graphic>
                  </JFXButton>
                  <JFXButton fx:id="addmember" buttonType="RAISED" onAction="#addMember" prefWidth="300.0" ripplerFill="#188630" styleClass="login-button" stylesheets="@../support/common.css" text="Save">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Courier New Bold" size="13.0" />
                     </font>
                     <effect>
                        <Glow />
                     </effect>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <graphic>
                        <FontAwesomeIconView glyphName="SAVE" size="20" />
                     </graphic>
                  </JFXButton>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
